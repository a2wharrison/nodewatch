#!/usr/bin/make -f
# Makefile wrapper

all: clean compile

clean:
	@(python build/scripts/mk clean %(local)s)

distclean: clean
	@(rm -dr lib)

compile:
	@(python build/scripts/mk compile %(local)s)

install:
	@(./install.sh)

uninstall:
	@(./uninstall.sh)

.PHONY: distclean clean compile uninstall install all
